{
  "metadata": [
    {
      "src": [
        {
            "files": [
                "*.sln"
            ],
            "exclude": [
                "**/obj/**",
                "**/bin/**",
                "..\\/**"
            ],
            "src": "./../../Source/Architectures/Any CPU/Krypton Components/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": true

    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "..\\/**"
        ]
      }
    ],
    "resource": [
        {
            "files": [
                "articles/images/**"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        },
        {
            "files": [
                "Logo.png",
                "Krypton.ico"
            ]
        }
    ],
    "overwrite": [
        {
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        }
    ],
    "globalMetadata": {
        "_appTitle": "Krypton Component Suite 2018\nBSD 3-Clause License\n© Component Factory Pty Ltd, 2006-2018, All rights reserved.",
        "_appFooter": "Modifications by Peter Wagner (aka Wagnerp) & Simon Coghlan (aka Smurf-IV) 2017 - 2018. All rights reserved. (https://github.com/Wagnerp/Krypton-NET-4.7)",
        "_appLogoPath": "Logo.png",
        "_appFaviconPath": "Krypton.ico",
        "_disableContribution": true
    },

    "dest": "..\\Output",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "docfx-tmpl/src"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}