{
  "metadata": [
    {
        "src": [
            {
                "files": [
                    "ComponentFactory.Krypton.Docking/ComponentFactory.Krypton.Docking 2019.csproj",
                    "ComponentFactory.Krypton.Navigator/ComponentFactory.Krypton.Navigator 2019.csproj",
                    "ComponentFactory.Krypton.Ribbon/ComponentFactory.Krypton.Ribbon 2019.csproj",
                    "ComponentFactory.Krypton.Toolkit/ComponentFactory.Krypton.Toolkit 2019.csproj",
                    "ComponentFactory.Krypton.Workspace/ComponentFactory.Krypton.Workspace 2019.csproj"
                // Do not add the TestApp
                ],
                "exclude": [
                    "**/obj/**",
                    "**/bin/**",
                    "..\\/**"
                ]
                ,"src": "./../../Source/Krypton Components/"
            }
        ],
        "dest": "api",
        "disableGitFeatures": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "..\\/**"
        ]
      }
    ],
    "resource": [
        {
            "files": [
                "articles/images/**"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        },
        {
            "files": [
                "Logo.png",
                "Krypton.ico"
            ]
        }
    ],
    "overwrite": [
        {
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        }
    ],
    "globalMetadata": {
        "_appTitle": "Krypton Component Suite 2019\nBSD 3-Clause License\n© Component Factory Pty Ltd, 2006-2019, All rights reserved.",
        "_appFooter": "<span>Modifications by Peter Wagner (aka Wagnerp) & Simon Coghlan (aka Smurf-IV) 2017 - 2019. All rights reserved. <a href=\"https://github.com/Wagnerp/Krypton-NET-5.470\">https://github.com/Wagnerp/Krypton-NET-5.470</a></span>",
        "_appLogoPath": "Logo.png",
        "_appFaviconPath": "Krypton.ico",
        "_disableContribution": true
    },

    "dest": "..\\Output",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "docfx-tmpl/src"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}